{
    "name": "hivokas/laravel-handlers",
    "description": "Request handlers for Laravel",
    "keywords": ["laravel", "handlers", "handler", "single", "action"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Sakovich",
            "email": "me@hivokas.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|6.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Hivokas\\LaravelHandlers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hivokas\\LaravelHandlers\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hivokas\\LaravelHandlers\\Providers\\HandlersServiceProvider"
            ]
        }
    }
}
